{
  "name": "vite-plugin-vue-termui",
  "private": false,
  "version": "0.0.4",
  "scripts": {
    "dev": "tsup -w",
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/*.mjs",
    "dist/*.d.ts"
  ],
  "dependencies": {
    "@vitejs/plugin-vue": "^2.2.0"
  },
  "peerDependencies": {
    "vue-termui": ">=0.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.0",
    "vue-termui": "workspace:*"
  }
}
