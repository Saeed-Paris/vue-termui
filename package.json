    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "execa": "^6.1.0",
    "globby": "^13.1.1",
    "minimist": "^1.2.5",
    "p-series": "^3.0.0",
    "semver": "^7.3.5",
    "tsup": "^5.11.13",
    "typescript": "^4.6.2",
    "unplugin-auto-import": "^0.6.4",
    "vite": "^2.8.6",
    "vitest": "^0.5.9",
    "vue": "^3.2.31",
    "vue-tsc": "^0.31.4"
