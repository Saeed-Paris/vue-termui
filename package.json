{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "url": "https://github.com/posva/vue-termui.git",
    "type": "git"
  },
  "scripts": {
    "release": "node scripts/release.mjs",
    "build": "pnpm run -r build --filter ./packages",
