{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@6.32.9",
  "repository": {
    "url": "https://github.com/posva-sponsors/vue-termui.git",
    "type": "git"
  },
  "scripts": {
    "release": "node scripts/release.mjs",
    "build": "pnpm run -r build --filter ./packages/core --filter ./packages/vite-plugin-vue-termui --filter ./packages/cli",
    "lint": "pnpm run -r lint",
    "types": "pnpm run -r types --filter ./packages/core",
    "test": "pnpm run -r test --parallel --filter ./packages",
    "docs": "pnpm run -r dev --filter ./packages/docs",
    "docs:build": "pnpm run -r build --filter ./packages/docs"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.29",
    "@vitejs/plugin-vue": "^2.3.1",
    "c8": "^7.11.2",
    "chalk": "^5.0.1",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "execa": "^6.1.0",
    "globby": "^13.1.1",
    "minimist": "^1.2.6",
    "p-series": "^3.0.0",
    "prettier": "^2.6.2",
    "semver": "^7.3.7",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.7.1",
    "vite": "^2.9.6",
    "vitest": "^0.10.0",
    "vue": "^3.2.33",
    "vue-tsc": "^0.34.10"
  }
}
